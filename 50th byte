
d segment    
          dw dup <?>
    var1 dw 1235h,1456h,1789h,1236h,1002h 
    res dw 5 dup(?)
d ends
c segment
    
    start:
        mov ax,d
        mov ds,ax 
        mov cx,5
        mov si,0 
        mov ax,0
        
    cmplt:
        mov ax,var1[si]
        neg ax
        mov res[si],ax
        add si,2
        loop cmplt:
     c ends
end start
